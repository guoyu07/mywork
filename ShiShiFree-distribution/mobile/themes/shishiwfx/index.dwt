window.onload = function(){
  $('#menu').css('display','');
}
$(function() {
	$('nav#menu').mmenu();
	$('#get_search_box').click(function(){
		$(".mm-page").children('div').hide();
		$("#main-search").css('position','fixed').css('top','0px').css('width','100%').css('z-index','999').show();
		//$('#keywordBox').focus();
	})
	$("#main-search .close").click(function(){
		$(".mm-page").children('div').show();
		$("#main-search").hide();
	})
});
</script>

<script src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
<script>
<!-- {/literal} -->
  wx.config({
    debug: false,
    appId: '{$signPackage.appId}',
    timestamp: '{$signPackage.timestamp}',
    nonceStr: '{$signPackage.nonceStr}',
    signature: '{$signPackage.signature}',
    jsApiList: [
        'onMenuShareTimeline',
        'onMenuShareAppMessage' 
    ]
  });
 wx.ready(function () {
	//ShiShiWFX监听“分享给朋友”
    wx.onMenuShareAppMessage({
      title: '{$page_title}',
      desc: '{$description}',
      link: '{$url}',
      imgUrl: 'http://ssgo.hanyu168.com/mobile/themes/shishiwfx/images/logo.gif',
      trigger: function (res) {	
		{if $url}
        alert('恭喜！分享可以获取提成哦！');
		{else}
		alert('糟糕，需要分销商登录才能获得提成哦！');
		{/if}
      },
      success: function (res) {
		{if $dourl }
        window.location.href="{$dourl}&type=1"; 
		{/if}
      },
      cancel: function (res) {
        alert('很遗憾，您已取消分享');
      },
      fail: function (res) {
        alert(JSON.stringify(res));
      }
    });

	//分享到朋友圈ShiShiWFX
    wx.onMenuShareTimeline({
      title: '{$page_title}',
      link: '{$url}',
      imgUrl: 'http://ssgo.hanyu168.com/mobile/themes/shishiwfx/images/logo.gif',
      trigger: function (res) {
			
        {if $url}
			alert('恭喜！分享可以获取提成哦！');
		{else}
			alert('糟糕，需要分销商登录才能获得提成哦！');
		{/if}
      },
      success: function (res) {
       	{if $dourl }
        window.location.href="{$dourl}&type=2"; 
		{/if}
      },
      cancel: function (res) {
         alert('很遗憾，您已取消分享');
      },
      fail: function (res) {
        alert(JSON.stringify(res));
      }
    });

});
<!-- {literal} -->
</script>

